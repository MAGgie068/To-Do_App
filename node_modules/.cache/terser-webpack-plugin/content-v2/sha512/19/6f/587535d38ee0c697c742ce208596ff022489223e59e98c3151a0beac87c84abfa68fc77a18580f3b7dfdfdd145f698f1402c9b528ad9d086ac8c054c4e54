                  />\n                </div>\n            </div>\n        </div>\n    );\n};\n\nconst TASKS_HEADER_MAPPING = {\n    INBOX: 'Inbox',\n    TODAY: 'Today',\n    NEXT: 'Next'\n}\n\nconst TaskItems = ({selectedTab, tasks}) => {\n    let tasksToRender = [...tasks];\n    if(selectedTab === 'NEXT'){\n        tasksToRender = tasksToRender.filter((task) => isAfter(task.date, new Date()) && isBefore(task.date, addDays(new Date(), 7)));\n    }\n\n    if(selectedTab === 'TODAY'){\n        tasksToRender = tasksToRender.filter((task) => isToday(task.date));\n    }\n\n    return (\n        <div className='task-item-container'>\n            {tasksToRender.map((task) => (\n                <div className='task-item'>\n                    <p>{task.text}</p>\n                    <p>{dateFnsFormat(new Date(task.date), FORMAT)}</p>\n                </div>\n            ))}\n        </div>\n    );\n};\n\nconst Tasks = ({selectedTab}) => {\n\n    const [showAddTask, setShowAddTask] = useState(false);\n    const [tasks, setTasks] = useState([]);\n\n    const addNewTask = (text, date) => {\n        const newTaskItem = {text, date: date || new Date()};\n        setTasks((prevState) => [...prevState, newTaskItem]);\n    };\n\n    return (\n        <div className='tasks'>\n            <h1>{TASKS_HEADER_MAPPING[selectedTab]}</h1>\n            {selectedTab === 'INBOX' ? <div className='add-task-btn' onClick={() => setShowAddTask((prevState) => !prevState)}>\n                <span className='plus'>+</span>\n                <span className='add-task-text'>Add Task</span>\n            </div> : null}\n            {showAddTask && <AddTask onAddTask={addNewTask} onCancel={() => setShowAddTask(false)} />}\n            {tasks.length > 0 ?\n            <TaskItems tasks={tasks} selectedTab={selectedTab} /> : (\n            <p>No Tasks</p>)}\n        </div>\n    );\n};\n\nexport default Tasks;\n","import React, {useState} from 'react'\nimport Sidebar from './Sidebar';\nimport Tasks from './Tasks';\n\nconst Content = () => {\n    const [selectedTab, setSelectedTab] = useState('INBOX');\n\n    return (\n        <section className='content'>\n            <Sidebar selectedTab={selectedTab} setSelectedTab={setSelectedTab} />\n            <Tasks selectedTab={selectedTab} />\n        </section>\n    )\n}\n\nexport default Content\n","import React from 'react';\n\nconst Footer = () => {\n    return (\n        <footer className='footer'>\n            <div>-by MAGgie068</div>\n        </footer>\n    )\n}\n\nexport default Footer\n","import React from 'react';\nimport Header from './components/Header';\nimport Content from './components/Content';\nimport Footer from './components/Footer';\nimport './App.scss';\n\nconst App = () => {\n    return <div>\n        <Header />\n        <Content />\n        <Footer />\n    </div>;\n};\n\nexport default App;","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport App from './App';\n\nReactDOM.render(<App />, document.getElementById('root'));"]}}